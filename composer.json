{
  "name": "maxonfjvipon/purple",
  "license": [
    "MIT"
  ],
  "authors": [
    {
      "name": "Max Trunnikov",
      "email": "mtrunnikov@gmail.com"
    }
  ],
  "require": {
    "php": ">=8.0",
    "ext-json": "*",
    "maxonfjvipon/elegant-elephant": "^4.0"
  },
  "require-dev": {
    "php-coveralls/php-coveralls": "^2.4",
    "phpstan/phpstan": "^0.12.64",
    "phpstan/phpstan-phpunit": "^0.12.17",
    "phpunit/phpunit": "^9.5"
  },
  "autoload": {
    "psr-4": {
      "Maxonfjvipon\\Purple\\": "src"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Maxonfjvipon\\Purple\\Tests\\": "tests"
    }
  },
  "minimum-stability": "dev",
  "prefer-stable": true,
  "config": {
    "preferred-install": {
      "*": "dist"
    },
    "sort-packages": true,
    "optimize-autoloader": true
  }
}
